<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- Hibernate������������������������ -->
<hibernate-mapping package="com.gps808.model">
	<class name="StandardUserVehiPermit" table="jt808_user_vehicle_permit">
		<!-- ��������������������������? -->
		<id	name="id" column="ID" type="integer" >
			<!-- ������������identity������������������ -->
			<generator class="identity"/>
		</id>
		<!-- ����������?������? -->
		<many-to-one name="vehicle" column="VehiIDNO" class="com.gps808.model.StandardVehicle" unique="true" lazy="false" not-found="ignore"/> 
		<many-to-one name="user" column="AccountID" class="com.gps808.model.StandardUserAccount" unique="true" lazy="false" not-found="ignore"/> 
	</class>
</hibernate-mapping>