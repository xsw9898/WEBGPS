		<!-- ������������ -->
        <id	name="id" column="id" type="integer" >
			<!-- ��������identity������������ -->
			<generator class="identity"/>
		</id>
		<!-- ������������ -->
        <property name="name" column="name" type="java.lang.String" length="40"/>
